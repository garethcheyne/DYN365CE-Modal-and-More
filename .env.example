# D365 Environment Configuration
# Copy this file to .env and fill in your values

# Your Dynamics 365 environment URL (required)
D365_URL=https://yourorg.crm.dynamics.com

# Authentication method: "interactive" or "clientsecret"
# - interactive: Opens browser for login (recommended for dev)
# - clientsecret: Uses app registration (for CI/CD)
D365_AUTH_METHOD=interactive

# For clientsecret auth only (leave empty for interactive)
D365_CLIENT_ID=
D365_CLIENT_SECRET=
D365_TENANT_ID=

# Solution settings
SOLUTION_NAME=err403UILib
SOLUTION_PUBLISHER_PREFIX=err403

# Set to "managed" or "unmanaged" for deployment
SOLUTION_TYPE=unmanaged
