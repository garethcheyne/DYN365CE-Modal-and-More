",fields:[new nR.Table({id:"table2",columns:[{id:"name",header:"Name",sortable:!0},{id:"score",header:"Score",sortable:!0}],data:[{id:1,name:"Zebra",score:85},{id:2,name:"Apple",score:92},{id:3,name:"Mango",score:78}],selectionMode:"none"})],buttons:[new nR.Button("Close",()=>{},!0)]}).show(),b(f," Sortable table - click column headers")}},{label:"Single Select",handler:()=>{new nR.Modal({title:"Single Selection",fields:[new nR.Table({id:"table3",columns:[{id:"product",header:"Product",sortable:!0},{id:"price",header:"Price"}],data:[{id:1,product:"Laptop",price:1200},{id:2,product:"Mouse",price:25}],selectionMode:"single",onRowSelect:e=>e.length&&b(f,` Selected: ${e[0].product}`)})],buttons:[new nR.Button("Close",()=>{},!0)]}).show(),b(f," Single selection table")}},{label:"Multi Select",handler:()=>{new nR.Modal({title:"Multi Selection",fields:[new nR.Table({id:"table4",columns:[{id:"task",header:"Task"},{id:"status",header:"Status"}],data:[{id:1,task:"Design",status:"Done"},{id:2,task:"Code",status:"In Progress"},{id:3,task:"Test",status:"Pending"}],selectionMode:"multiple",onRowSelect:e=>b(f,` ${e.length} row(s) selected`)})],buttons:[new nR.Button("Close",()=>{},!0)]}).show(),b(f," Multi selection table")}}],T=[{label:"Modal + Toast",handler:()=>{nR.Modal.open({title:"Integration Test",message:"Click OK to show a toast",buttons:[new nR.Button("OK",()=>{nR.Toast.success({title:"Success",message:"Modal closed, toast shown!"}),b(m," Modal closed, toast displayed")},!0)]}),b(m," Integration modal opened")}},{label:"Lookup in Modal",handler:()=>{nR.Modal.open({title:"Select Account",fields:[new nR.Input({id:"selected",label:"Selected Account",disabled:!0})],buttons:[new nR.Button("Open Lookup",()=>(nR.Lookup.open({entity:"account",columns:["name"],onSelect:e=>{b(m,` Lookup in modal: ${e[0]?.name||"none"}`)}}),!1),!0),new nR.Button("Close",()=>{})]}),b(m," Modal with lookup opened")}},{label:"Run All Auto Tests",handler:()=>{b(m," Running automated tests...","info");n